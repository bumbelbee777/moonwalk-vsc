{
    "name": "moonwalk-vsc",
    "displayName": "MoonWalk VScode extension",
    "description": "Syntax highlighting for MoonWalk",
    "version": "3",
    "publisher": "MitkoYT",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "moonwalk",
            "aliases": ["MoonWalk", "moonwalk"],
            "extensions": [".mw"],
            "configuration": "./language-configuration.json",
            "icon": {
                "light": "light.png",
                "dark": "dark.png"
            }
        }],
        "grammars": [{
            "language": "moonwalk",
            "scopeName": "source.mw",
            "path": "./syntaxes/moonwalk.tmLanguage.json"
        }]
    }
}
